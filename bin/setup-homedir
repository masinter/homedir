#!/bin/sh

cd ~
echo setup ~/bin
rm -rf bin
cp -pr il/homedir/bin bin
ls -l bin

echo setup ~/.emacs
rm -f .emacs
cp -p il/homedir/dot.emacs.el .emacs
ls -l .emacs

echo setup ~/.bashrc
rm -f .bashrc
cp -p il/homedir/dot.bashrc .bashrc
ls -l .bashrc

echo setup ~/.bash_aliases
rm -f .bash_aliases
cp -p il/homedir/dot.bash_aliases .bash_aliases
ls -l .bash_aliases

echo setup ~/.profile
rm -f .profile .bash-profile
cp -p il/homedir/dot.profile .profile
cp -p il/homedir/dot.bash_profile .bash_profile
ls -l .profile .bash_profile


echo setup INIT
rm -f il/INIT
cd il
cp -p homedir/INIT INIT
ls -l homedir/INIT







