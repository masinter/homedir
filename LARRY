(DEFCOMMAND "ED" (&REST LINE)
   (IF (EQ (CAR (LISTP LINE))
           'WHERE)
       THEN (MASTERSCOPE (CONS 'EDIT LINE))
     ELSEIF (AND (NULL (CDR LINE))
                 (LISTP (CAR LINE)))
       THEN (APPLY 'ED (CAR LINE))
     ELSE (CL:APPLY #'ED LINE)))

(DEFCOMMAND "SHOW" (&REST LINE)
   (MASTERSCOPE (CONS 'SHOW LINE)))

(DEFCOMMAND "WHO" (&REST LINE)
   (MASTERSCOPE (CONS 'WHO LINE)))
