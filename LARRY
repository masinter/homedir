(LOWESTVERSION
  [LAMBDA (X)                                              (* ; "Edited  2-Jun-2022 20:14 by larry")
    (LET ((VERS (DIRECTORY (PACKFILENAME.STRING 'VERSION '* 'BODY X)))
          (MIN 9999991))
         (for Y in VERS bind Z when (< (SETQ Z (FILENAMEFIELD Y 'VERSION))
                                       MIN) do (SETQ MIN Z))
         MIN])

