                       (FMEMB (TYPENAME (GETTOPVAL X))
                              '(FONTCLASS FONTDESCRIPTOR))
                       (NOT (ASSOC X FONTVARS)
                            (NOT (FMEMB X NEWFONTVARS)))
                       (PUSH NEWFONTVARS X])

